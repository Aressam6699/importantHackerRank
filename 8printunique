def merge_the_tools(string, k):
# your code goes here
    v=[]
    for i in range(len(string)):
        if(i%k == 0 and i!=0):
            print()
            v.clear()
        if(string[i] not in v):
            print(string[i],end="")
            v.append(string[i])

if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
